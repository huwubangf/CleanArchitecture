{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithThreadId", "WithProcessId" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] ({RequestId}) {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=BANG-HUU\\SQLEXPRESS;Initial Catalog=CC;Integrated Security=True;Trust Server Certificate=True",
    "Redis": "localhost:6379"
  },
  "JwtSettings": {
    "Issuer": "yourapp",
    "Audience": "yourapp",
    "SecretKey": "super-secret-key-1234567890-EXTRA-CHARS",
    "ExpiryMinutes": 30
  }
}
